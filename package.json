  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json --target es2018 --outDir lib",
    "bench": "ts-node --require tsconfig-paths/register benches/index.ts",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/structures": "^0.11.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "^29.2.4",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "husky": "^4.3.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "pretty-bytes": "5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.4"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.1.8",
    "@mongodb-js/zstd": "^1.0.0",
    "extra-benchmark": "^0.2.2",
    "extra-filesystem": "^0.4.8",
    "extra-promise": "^4.4.0",
    "extra-utils": "^3.5.1",
